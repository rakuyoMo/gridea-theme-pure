<html>

<head>
    <%- include('./includes/head', { siteTitle: `收藏列表 | ${themeConfig.siteName}` }) %>
</head>

<body>
<div class="main gt-bg-theme-color-first">
        <div class="main-content">
            <%- include('./includes/header') %>
            <div class="post-container">
                <div class="post-detail">
                    <article>
                        <!-- <div class="post-content-wrapper"> -->
                            <div class="post-content">
                                <div class="references-desc">
                                    <%- site.customConfig.descreference %>
                                </div>
                                <h3 id="linklist">收藏列表</h3>
                                <% if (site.customConfig.references) { %>
                                    <% site.customConfig.references.forEach(function(reference) { %>
                                        <div class="reference-box">
                                            <div class="flink-info">
                                                <% if(reference.siteLink == "/referencessq") { %>
                                                    <a class="gt-c-content-color-first" href="<%= reference.siteLink %>" target="_Self"><%= reference.siteName %></a>
                                                    <% } else { %>
                                                        <a class="gt-c-content-color-first" href="<%= reference.siteLink %>" target="_blank"><%= reference.siteName %></a>
                                                        <% } %>
                                                <br />
                                                <div class="flink-info-desc gt-c-content-color-first">
                                                    <%= reference.description %>
                                                </div>
                                            </div>
                                        </div>
                                    <% }); %>
                                <% } else { %>
                                    暂无收藏。
                                <% } %>
                            </div>
                        <!-- </div> -->
                    </article>
                </div>
                <% if (typeof commentSetting !== 'undefined' && commentSetting.showComment) { %>
                    <% if (commentSetting.commentPlatform === 'gitalk') { %>
                        <%- include('./includes/gitalk') %>
                    <% } %>

                    <% if (commentSetting.commentPlatform === 'disqus') { %>
                        <%- include('./includes/disqus') %>
                    <% } %>
                <% } %>
            </div>



            <%- include('./includes/footer') %>
        </div>
    </div>
</body>

</html>
